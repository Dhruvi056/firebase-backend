{
  "version": 2,
  "framework": "create-react-app",

  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "devCommand": "npm start",

  "functions": {
    "api/forms/[formId].js": {
      "maxDuration": 10
    },
    "api/f/[formId].js": {
      "maxDuration": 10
    }
  },

  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
