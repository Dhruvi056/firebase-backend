{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "devCommand": "npm start",
  "framework": "create-react-app",

  "functions": {
    "api/forms/[formId].js": {
      "runtime": "nodejs16.x",
      "maxDuration": 10
    }
  },

  "rewrites": [
    {
      "source": "/forms/:formId",
      "destination": "/api/forms/:formId"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
